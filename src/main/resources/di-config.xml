<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="A3" class="net.rezss.springframework.model.Auto">
        <property name="name" value="A3"></property>
        <property name="kraftstoff" ref="benzin"></property>
    </bean>

    <bean id="benzin" class="net.rezss.springframework.model.fuel.Benzin" scope="prototype"/>
</beans>